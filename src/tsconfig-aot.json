{
	"compilerOptions": {
		"target": "es5",
		"module": "es2015",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [ "es2015", "dom"],
		"removeComments": true,
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,
		"outDir": "aot"
	},
	"files": [
		"src/app/app.module.ts",
		"src/main.ts"
	],
	"angularCompilerOptions": {
		"genDir": "aot",
		"skipMetadataEmit": true
	}
}