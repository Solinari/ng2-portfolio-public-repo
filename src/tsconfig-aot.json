{
	"compilerOptions": {
		"target": "es5",
		"module": "es2015",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"removeComments": false,
		"noImplicitAny": false,
		"lib": [ "es2015", "dom"],
		"outDir": "dist"
	},
	"files": [
		"src/app.module.ts",
		"src/main.ts"
	],
	"angularCompilerOptions": {
		"genDir": "aot",
		"skipMetadataEmit": true
	}
}